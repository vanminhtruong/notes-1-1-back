{
  "name": "expressjs-starter",
  "version": "1.0.0",
  "description": "Express.js project configured with best practices",
  "main": "src/server.js",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "nodemon src/server.js",
    "start": "node src/server.js",
    "build": "echo 'Build completed - Node.js app ready'",
    "postinstall": "npm run setup:production",
    "setup:production": "node -e \"if(process.env.NODE_ENV==='production'){console.log('Production setup completed')}\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "test": "jest --runInBand",
    "test:watch": "jest --watch"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.11.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "google-auth-library": "^10.3.0",
    "helmet": "^7.0.0",
    "http-errors": "^2.0.0",
    "joi": "^17.13.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^7.0.5",
    "sequelize": "^6.37.3",
    "socket.io": "^4.8.1",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.3.3",
    "supertest": "^6.3.4"
  }
}
